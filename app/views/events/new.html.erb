
<div class="main-container" >
<%= simple_form_for @event do |f| %>
  <%= f.input  :name %>
  <%= f.input  :description %>
  <%= f.input  :location %>
  <%= f.input  :people_needed %>
  <%= f.input  :photo %>
  <%= f.input :start_time, as: :datetime%>
  <%= f.input :end_time, as: :datetime %>
  <%= f.button :submit %>
<% end %>
 </div>
